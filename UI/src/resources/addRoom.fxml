<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddRoom">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="399.0" style="-fx-background-color: #C5E1A5;" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="97.0" layoutY="77.0" text="ROOM NAME" />
            <Label layoutX="97.0" layoutY="126.0" text="ROOM CAPACITY" />
            <Label layoutX="97.0" layoutY="176.0" text="DISABILITY FRIENDLINESS" />
            <Label layoutX="97.0" layoutY="233.0" text="IS LAB" />
            <TextField fx:id="roomName" layoutX="320.0" layoutY="73.0" promptText="Room Name" />
            <TextField fx:id="capacity" layoutX="320.0" layoutY="122.0" promptText="Room Capacity" />
            <RadioButton fx:id="disYes" layoutX="320.0" layoutY="176.0" mnemonicParsing="false" text="YES" />
            <RadioButton fx:id="isLabYes" layoutX="320.0" layoutY="233.0" mnemonicParsing="false" text="YES" />
            <RadioButton fx:id="disNo" layoutX="429.0" layoutY="176.0" mnemonicParsing="false" text="NO" />
            <RadioButton fx:id="isLabNo" layoutX="429.0" layoutY="233.0" mnemonicParsing="false" text="NO" />
            <Button layoutX="108.0" layoutY="325.0" mnemonicParsing="false" onAction="#backButt" style="-fx-background-color: #FF5252;" text="CANCEL" textFill="WHITE" />
            <Button layoutX="408.0" layoutY="325.0" mnemonicParsing="false" onAction="#submitt" text="ADD" />
         </children>
    </AnchorPane>
  </children>
</VBox>
