<VBox prefHeight="700.0" prefWidth="900.0" style="-fx-background-color: #c5e1a5;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ManageTT">
