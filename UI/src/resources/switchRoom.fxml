<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="501.0" prefWidth="920.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SwitchRoomController">

    <children>

        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="459.0" prefWidth="883.0" style="-fx-background-color: #C5E1A5;" VBox.vgrow="ALWAYS">

            <children>

                <TableView fx:id="timetable" layoutX="30.0" layoutY="104.0" prefHeight="146.0" prefWidth="876.0">

                    <columns>
                        <TableColumn fx:id="dayColumn" prefWidth="75.0" text="Day" />

                        <TableColumn fx:id="c1" editable="false" prefWidth="75.0" sortable="false" text="7:00-8:00" />

                        <TableColumn fx:id="c2" editable="false" prefWidth="75.0" sortable="false" text="8:00-9:00" />

                        <TableColumn fx:id="c3" editable="false" prefWidth="75.0" sortable="false" text="9:00-10:00" />

                        <TableColumn fx:id="c4" editable="false" prefWidth="75.0" sortable="false" text="10:00-11:00" />

                        <TableColumn fx:id="c5" editable="false" prefWidth="75.0" sortable="false" text="11:00-12:00" />

                        <TableColumn fx:id="c6" prefWidth="75.0" sortable="false" text="12:00-1:00" />

                        <TableColumn fx:id="c7" prefWidth="75.0" sortable="false" text="1:00-2:00" />

                        <TableColumn fx:id="c8" prefWidth="75.0" sortable="false" text="2:00-3:00" />

                        <TableColumn fx:id="c9" prefWidth="75.0" sortable="false" text="3:00-4:00" />

                        <TableColumn fx:id="c10" prefWidth="75.0" sortable="false" text="4:00-5:00" />

                        <TableColumn fx:id="c11" prefWidth="75.0" sortable="false" text="5:00-6:00" />


                    </columns>

                    <columnResizePolicy>

                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />

                    </columnResizePolicy>

                </TableView>

                <ComboBox fx:id="course" editable="true" layoutX="109.0" layoutY="43.0" onAction="#selectCourse" prefWidth="150.0" promptText="-" />

                <Label layoutX="47.0" layoutY="47.0" text="Course" />



                <Label layoutX="55.0" layoutY="359.0" text="Day" />

                <ComboBox layoutX="109.0" layoutY="355.0" onAction="#selectCourse" prefWidth="150.0" promptText="MONDAY" visibleRowCount="7" />

                <ComboBox layoutX="549.0" layoutY="355.0" prefWidth="150.0" />

                <Label layoutX="442.0" layoutY="359.0" text="Available Rooms" />

                <Button layoutX="154.0" layoutY="412.0" mnemonicParsing="false" onAction="#backButt" style="-fx-background-color: #ff5252;" text="CANCEL" textFill="WHITE" />

                <Button layoutX="601.0" layoutY="412.0" mnemonicParsing="false" text="OKAY" />
                <ComboBox fx:id="year" editable="true" layoutX="653.0" layoutY="43.0" onAction="#selectYear" prefWidth="150.0" promptText="" />
                <Label layoutX="598.0" layoutY="47.0" text="Year" />

            </children>

        </AnchorPane>

    </children>
</VBox>
